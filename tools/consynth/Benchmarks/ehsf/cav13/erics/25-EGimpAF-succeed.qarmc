init(Y,C,P,PC):=Y=5,C=0,P=0,PC=1.
next(Y,C,P,PC,Yp,Cp,Pp,PCp):=
    PC=1,Yp=Y,Cp=1,Pp=P,PCp=2;
    PC=2,Yp=Y,Cp=C,Pp=P,PCp=3;
    PC=2,Yp=Y,Cp=C,Pp=P,PCp=4;
    PC=3,Y>=0,Yp=Y-1,Cp=1,Pp=P,PCp=3;
    PC=3,Y<0,Cp=C,Pp=1,PCp=4.
dst(Y,C,P,PC):=P>0.
% EG p1
phi0(Y,C,P,PC):-init(Y,C,P,PC).
exists([Yp,Cp,Pp,PCp],phi0(Yp,Cp,Pp,PCp)):-phi0(Y,C,P,PC),next(Y,C,P,PC,Yp,Cp,Pp,PCp).
p1(Y,C,P,PC):-phi0(Y,C,P,PC).

skolem_template(s3, [Y,C,P,PC,Yp,Cp,Pp,PCp],
 		true,
		true,
% 		(PC=1 ; B=0,PC=2 ; B=1,PC=2 ; PC=3 ),
 		(
		  PC=1,Yp=Y,Cp=1,Pp=P,PCp=2;
		  B=0,PC=2,Yp=Y,Cp=C,Pp=P,PCp=3;
		  B=1,PC=2,Yp=Y,Cp=C,Pp=P,PCp=4;
		  PC=3,Y>=0,Yp=Y-1,Cp=1,Pp=P,PCp=3;
		  PC=3,Y<0,Cp=C,Pp=1,PCp=4
 		),
 		(B=0;B=1)
 	       ).
% p1 /\ c=1 => AF p>0
phi1(Y,C,P,PC):-p1(Y,C,P,PC),C=1.
exists([Yp,Cp,Pp,PCp],next(Y,C,P,PC,Yp,Cp,Pp,PCp)):-phi1(Y,C,P,PC),\+dst(Y,C,P,PC).
phi1(Yp,Cp,Pp,PCp):-phi1(Y,C,P,PC),next(Y,C,P,PC,Yp,Cp,Pp,PCp),\+dst(Y,C,P,PC).
rank(Y,C,P,PC,Yp,Cp,Pp,PCp):-phi1(Y,C,P,PC),next(Y,C,P,PC,Yp,Cp,Pp,PCp),\+dst(Y,C,P,PC).
wf(rank/8).

query_naming(init(y,c,p,pc)).
query_naming(phi0(y,c,p,pc)).
