    PC=1,CS>0,Cp=C,Rp=R,CSp=CS,PCp=2;
    B=0,PC=2,Cp=C,Rp=R,CSp=CS,PCp=3;
    B=1,PC=2,Cp=C,Rp=R,CSp=CS,PCp=4;
    PC=3,Cp=C-1,Rp=R+1,CSp=CS-1,PCp=1;
    PC=4,C<CS,Cp=C,Rp=R,CSp=CS-1,PCp=1.
dst(C,R,CS,PC):=R>=6.

