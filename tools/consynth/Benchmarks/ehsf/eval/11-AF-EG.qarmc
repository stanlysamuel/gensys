init(A,B,C):=A=0,B=5,C=1.
step(A,B,C,D,E,F):=
 B>=0,D=A,E=B,C=1,F=2;
 E=B-1,D=A,C=2,F=1;
 B<0,D=A,E=B,C=1,F=3;
 D=1,E=B,C=3,F=4;
 D=0,E=B,C=4,F=4;
 D=A,E=B,C=4,F=4.
phi0(A,B,C):-init(A,B,C).
exists([D,E,F],step(A,B,C,D,E,F)):-phi0(A,B,C),\+p1(A,B,C).
phi0(D,E,F):-phi0(A,B,C),step(A,B,C,D,E,F),\+p1(A,B,C).
rel0(A,B,C,D,E,F):-phi0(A,B,C),\+p1(A,B,C),step(A,B,C,D,E,F),phi0(D,E,F),\+p1(D,E,F).
trel0(A,B,C,D,E,F):-rel0(A,B,C,D,E,F).
trel0(A,B,C,G,H,I):-trel0(A,B,C,D,E,F),rel0(D,E,F,G,H,I).
dwf(trel0(A,B,C,D,E,F)).
phi1(A,B,C):-p1(A,B,C).
exists([D,E,F],phi1(D,E,F)):-phi1(A,B,C), step(A,B,C,D,E,F).
A>0 :- phi1(A,B,C).

