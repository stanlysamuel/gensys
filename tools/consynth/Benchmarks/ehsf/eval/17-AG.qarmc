p0(A,B,C,D):=A>=1,B>=1,C=1,D=1.
fds_transrel(A,B,C,D,E,F,G,H):=A>=1,E=A,F=B,C=1,G=2,H=D;E=A-1,F=B,C=2,G=1,H=D;A=<0,E=A,F=B,C=1,G=3,H=D;B>=1,E=A,F=B,D=1,H=2,G=C;E=A,F=B-1,D=2,H=1,G=C;B=<0,E=A,F=B,D=1,H=3,G=C.
phi0(A,B,C,D):-p0(A,B,C,D).
phi0(E,F,G,H):-phi0(A,B,C,D),fds_transrel(A,B,C,D,E,F,G,H).
(C=<3;A=0),(D=<3;B=0):-phi0(A,B,C,D).
