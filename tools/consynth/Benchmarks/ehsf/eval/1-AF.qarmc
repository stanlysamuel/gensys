p0(A,B,C):=A=0,B>0,C=1.
fds_transrel(A,B,C,D,E,F):=B>0,D=A,E=B,C=1,F=2;E=B-1,D=A,C=2,F=1;B=<0,D=A,E=B,C=1,F=3;D=1,E=B,C=3,F=4.
