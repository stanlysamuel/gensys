init(X,Y):=X=0,Y=0.
adam(X,Y,Xp,Yp):=
    X=Xp-1,Y=Yp;
    X=Xp+1,Y=Yp;
    X=Xp,Y=Yp.
eve(X,Y,Xp,Yp):=
    X=Xp,Y=Yp+1;
    X=Xp,Y=Yp-1;
    X=Xp,Y=Yp.
safe(X,Y):=Y=<X+1,Y>=(X-1).
% init => (c>5 /\ AG r=<5)
phi0(X,Y):-init(X,Y).
exists([Xpp,Ypp], (phi0(Xpp,Ypp))):-adam(X,Y,Xp,Yp),phi0(X,Y).
safe(Xp,Yp):-adam(X,Y,Xp,Yp),phi0(X,Y).
safe(X,Y):-phi0(X,Y).
skolem_template(s3, [X,Y,Xp,Yp,Xpp,Ypp],
	true,
	true,
	(
    		Yp-Xp>=1,Xp=Xpp,Ypp=Yp+T1;
		Yp-Xp=1,Xp=Xpp,Ypp=Yp+T4;
		Yp-Xp=(-1),Xp=Xpp,Ypp=Yp+T5;
		Yp-Xp=<(-1),Xp=Xpp,Ypp=Yp+T2;
		Yp-Xp=0,Xp=Xpp,Ypp=Yp+T3;
		Yp-Xp>=2,Xp=Xpp,Ypp=Yp+T6;
		Yp-Xp=<(-2),Xp=Xpp,Ypp=Yp+T7;
		Yp-Xp>=3,Xp=Xpp,Ypp=Yp+T8;
		Yp-Xp=<(-3),Xp=Xpp,Ypp=Yp+T9
		
	),
	(T1>=(-1),T1=<1,T2>=(-1),T2=<1,T3>=(-1),T3=<1,T4>=(-1),T4=<1,T5>=(-1),T5=<1,T6>=(-1),T6=<1,T7>=(-1),T7=<1,T8>=(-1),T8=<1,T9>=(-1),T9=<1)
	).


query_naming(phi0(x,y)).

